<div class="row items-push">   
  <div class="block-header block-header-default">
    <h3 class="block-title">
    <a class="fw-semibold text-dual" href="https://www.netwerthcard.com/">
      <!-- <span class="smini-visible">
        <i class="fa fa-circle-notch text-primary"></i>
      </span> -->
      <strong class="fw-bold">Netwerth</strong><strong class="fw-medium">Card</strong>
    </a> - Payout History
   </h3>
  </div>     
  <div class="row text-center">
    <div class="col-12 col-lg-3">
      <a class="block block-rounded block-link-shadow text-center">
        <div class="block-content block-content-full">
          <div class="fs-2 fw-semibold text-success"><i class="fa fa-2x fa-check-circle text-success"></i></div>
        </div>
        <div class="block-content py-2">
          <p class="fw-medium fs-sm text-muted mb-0">
            Payouts For You
          </p>
        </div>
      </a>
    </div>

    <div class="col-12 col-lg-3">
      <a class="block block-rounded block-link-shadow text-center">
        <div class="block-content block-content-full">
          <div class="fs-2 fw-semibold text-success"><i class="fa fa-2x fa-minus-circle text-warning" ></i></div>
        </div>
        <div class="block-content py-2">
          <p class="fw-medium fs-sm text-muted mb-0">
            Payouts For Others
          </p>
        </div>
      </a>
    </div>
    <div class="col-12 col-lg-3">
      <a class="block block-rounded block-link-shadow text-center">
        <div class="block-content block-content-full">
          <div class="fs-2 fw-semibold text-success"><i class="fa fa-2x fa-calendar-check"></i></div>
        </div>
        <div class="block-content py-2">
          <p class="fw-medium fs-sm text-muted mb-0">
            Payout Received
          </p>
        </div>
      </a>
    </div>
    <div class="col-12 col-lg-3">
      <a class="block block-rounded block-link-shadow text-center">
        <div class="block-content block-content-full">
          <div class="fs-2 fw-semibold text-warning"><i class="fa fa-2x fa-calendar-xmark"></i></div>
        </div>
        <div class="block-content py-2">
          <p class="fw-medium fs-sm text-muted mb-0">
            Payout Pending
          </p>
        </div>
      </a>
    </div>
  </div>
  <% @payouts.each_with_index do |payout, indx| %>
    <div class="row items-push text-center">
      <div class="block-header block-header-default">
        <h3 class="block-title"><%= payout['payoutID'][3..payout['payoutID'].size] %></h3>
      </div>
      <div class="col-12 col-lg-3">
        <a class="block block-rounded block-link-shadow text-center">
          <div class="block-content block-content-full">
            <div class="fs-2 fw-semibold text-success"><%= number_to_percentage(payout['investedForUserX']/payout['investedTotal'] * 100) %></div>
          </div>
          <div class="block-content py-2 bg-success-light">
            <p class="fw-medium fs-sm text-muted mb-0">
              Your Ownership
            </p>
          </div>
        </a>
      </div>
      <div class="col-12 col-lg-3">
        <a class="block block-rounded block-link-shadow text-center">
          <div class="block-content block-content-full">
            <div class="fs-2 fw-semibold text-success"><%= number_to_currency(((payout['investedForUserX']/payout['investedTotal']) * (payout['payoutTotal'] * 0.01)), precision: 2)%></div>
          </div>
          <div class="block-content py-2 bg-success-light">
            <p class="fw-medium fs-sm text-muted mb-0">
              Your Payout
            </p>
          </div>
        </a>
      </div>
      <div class="col-12 col-lg-3">
        <a class="block block-rounded block-link-shadow text-center">
        	<% if payout['returnOnInvestmentPercentage'].negative? %>
          	<div class="block-content block-content-full">
              <div class="fs-2 fw-semibold text-warning"><%= number_to_percentage(payout['returnOnInvestmentPercentage'] * 100, precision: 2)%>
              </div>
            </div>
            <div class="block-content py-2 bg-warning-light">
              <p class="fw-medium fs-sm text-muted mb-0">
                Return On Investment
              </p>
            </div>
        	<% else %>
            <div class="block-content block-content-full">
              <div class="fs-2 fw-semibold text-success"><%= number_to_percentage(payout['returnOnInvestmentPercentage'] * 100, precision: 2)%>
              </div>
            </div>
            <div class="block-content py-2 bg-success-light">
              <p class="fw-medium fs-sm text-muted mb-0">
                ROI
              </p>
            </div>
          <% end %>
        </a>
      </div>
      <div class="col-12 col-lg-3">
        <a class="block block-rounded block-link-shadow text-center">
          <div class="block-content block-content-full">
            <div class="fs-2 fw-semibold text-success"><%= number_to_currency(payout['payoutTotal'] * 0.01, precision: 2) %></div>
          </div>
          <div class="block-content py-2 bg-success-light">
            <p class="fw-medium fs-sm text-muted mb-0">
              Group Payout
            </p>
          </div>
        </a>
      </div>
      <div class="col-12 col-lg-3">
        <a class="block block-rounded block-link-shadow text-center">
          <div class="block-content block-content-full">
            <div class="fs-2 fw-semibold text-dark"><%= payout['numberOfInvestors']%></div>
          </div>
          <div class="block-content py-2 bg-body-light">
            <p class="fw-medium fs-sm text-muted mb-0">
              Number of Investors
            </p>
          </div>
        </a>
      </div>

    		
    		
      <div class="col-12 col-lg-3">
        <a class="block block-rounded block-link-shadow text-center">
          <div class="block-content block-content-full">
            <div class="fs-2 fw-semibold text-dark"><%= number_to_currency(payout['netTotal'],precision: 2) %></div>
          </div>
          <div class="block-content py-2 bg-body-light">
            <p class="fw-medium fs-sm text-muted mb-0">
              Group Deposit Total
            </p>
          </div>
        </a>
      </div>
      <div class="col-12 col-lg-3">
        <a class="block block-rounded block-link-shadow text-center">
          <div class="block-content block-content-full">
            <div class="fs-2 fw-semibold text-dark"><%= number_to_currency(payout['asideToSpend'] ,precision: 2) %></div>
          </div>
          <div class="block-content py-2 bg-body-light">
            <p class="fw-medium fs-sm text-muted mb-0">
              Group Spending Total
            </p>
          </div>
        </a>
      </div>
      <div class="col-12 col-lg-3">
        <a class="block block-rounded block-link-shadow text-center">
          <div class="block-content block-content-full">
            <div class="fs-2 fw-semibold text-dark"><%= number_to_currency((payoutInvesmentPrinciple = payout['investedTotal']),precision: 2) %></div>
          </div>
          <div class="block-content py-2 bg-body-light">
            <p class="fw-medium fs-sm text-muted mb-0">
              Group Investment Total
            </p>
          </div>
        </a>
      </div>
  	  <div class="block-content">
  	    <table class="table table-borderless table-striped table-vcenter fs-sm">
  	    	<thead>
            <tr>
              <th class="text-center d-none d-sm-table-cell">&nbsp;</th>
              <th class="text-center d-none d-sm-table-cell">Ownership</th>
              <th class="text-center d-none d-sm-table-cell">Deposited</th>
              <th class="text-center">Invested</th>
              <th class="text-center">Payout</th>
              <th class="text-center">Received</th>
              
            </tr>
          </thead>
          
      		

  	    	
  	    	<% payout['deposits'].each do |chargeX| %>
            <% chargeXFiltered = Charge.paymentIntentNet(chargeX['id']) %>
  	    		<% chargeXChargeNet = chargeXFiltered[:net] * 0.01 %>

  	    		<% investedFromchargeX = chargeXChargeNet * (chargeX['metadata']['percentToInvest'].to_i * 0.01) %>
  		      <% ownershipForchargeX = ((investedFromchargeX/(payout['investedTotal']))) %>

  		      <tbody>
  		        <tr>
  		          <td class="text-center d-none d-sm-table-cell">
    		          <% if chargeX['customer'] == current_user&.stripeCustomerID %>
  			            <div class="text-success">
  			              <i class="fa fa-2x fa-check-circle"></i>

  			            </div>
    		          <% else %>
    			            <div class="text-warning">
    			              <i class="fa fa-2x fa-minus-circle" ></i>
    			            </div>
    			        <% end %>
                  
  		          </td>
  		          <td class="text-center d-none d-sm-table-cell">
  		            <a><%= number_to_percentage(ownershipForchargeX * 100, precision: 2) %></a>
  		          </td>
  		          <td class="text-center d-none d-sm-table-cell">
  		            <a><%= number_to_currency(chargeXChargeNet, precision: 2)%></a>
  		          </td>
  		          <td class="text-center">
  		            <a><%= number_to_currency(investedFromchargeX, precision: 2)%></a>
  		          </td>
  		          <td class="text-center">
  		            <a><%= number_to_currency(((ownershipForchargeX * 0.01) *  payout['payoutTotal']), precision: 2) %></a>
  		          </td>


                <td class="text-center">
                  <% if chargeX['metadata']['payout'] == true.to_s %>
                    <div class="text-success">
                      <i class="fa fa-2x fa-calendar-check"></i>

                    </div>
                  <% else %>
                      <div class="text-warning">
                        <i class="fa fa-2x fa-calendar-xmark"></i>
                      </div>
                  <% end %>
                </td>

  		        </tr>
  		      </tbody>
  	      <% end %>
  	    </table>
  	  </div>
    </div>
    <div class="block-header block-header-default">
      <h3 class="block-title">
      <a class="fw-semibold text-dual" href="https://www.netwerthcard.com/">
        <!-- <span class="smini-visible">
          <i class="fa fa-circle-notch text-primary"></i>
        </span> -->
        <strong class="fw-bold">Netwerth</strong><strong class="fw-medium">Card</strong>
      </a> - Payout History
     </h3>
    </div>  
  <% end %>
</div>        