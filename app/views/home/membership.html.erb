<div class="page-content header-clear-medium" style="min-height: 511px;">
                
  <div class="card card-style">
      <div class="content">
      	<% if current_user.member? %>
          <div class="d-flex">
              <div class="align-self-center pr-3">
                  <i class="fa fa-check-circle color-green1-dark fa-5x"></i>
              </div>
              <div class="align-self-center">
                  <h4>Valid Membership</h4>
                  <p class="line-height-s">
                      Your pack will expires <strong>15th of May, 2025</strong>. It will renew automatically on that day.
                  </p>
              </div>
          </div>
        <% else %>
          <div class="d-flex mb-4">
            <div class="align-self-center pr-3">
              <i class="fa fa-times-circle color-red1-dark fa-2x"></i>
            </div>
            <div class="align-self-center">
              <h4>No Membership</h4>
              <p class="line-height-s">Members save on every transaction fee! This helps keep us in business.</p>
            </div>
          </div>
          <div class="divider w-100 mt-2 mb-2 divider-margins"></div>
          <div class="d-flex mb-4">
            <div class="align-self-center pr-3">
              <i class="fa fa-percent color-red1-dark fa-2x"></i>
            </div>
            <div class="align-self-center">
              <h4>35% Standard Fee</h4>
              <p class="line-height-s">Your monthly membership helps reduce the standard transaction fee!</p>
            </div>
          </div>
        <% end %>
      </div>
  </div>
  
  	
