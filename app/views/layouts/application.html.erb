    <div id="menu-transaction-request" class="menu menu-box-bottom menu-box-detached rounded-m" data-menu-height="440" style="height: 440px; display: block;">
      <div class="menu-title"><h1>New Bank Source</h1><p class="color-highlight">Enter Bank Details</p><a href="#" class="close-menu"><i class="fa fa-times"></i></a></div>

      <div class="divider divider-margins mt-3 mb-3"></div>
        <%= form_for :stripeToken, url: stripe_tokens_path do |f| %>
          <div class="input-style input-style-2 has-icon input-required mt-4">
            <span class="color-highlight input-style-1-active" for="stripeToken_account_holder_type">Company or Personal Account?</span>
            <select class="form-select" id="stripeToken_account_holder_type" name="stripeToken[account_holder_type]" required="true">
              <option value="individual">Personal</option>
              <option value="company">Company</option>
            </select>
          </div>
          <div class="input-style input-style-2 has-icon input-required mt-4">
              <i class="input-icon fa fa-user"></i>
              <span class="color-highlight input-style-1-active">Full Legal Name</span>
              <em>(required)</em>
              <%= f.text_field :account_holder_name, class: 'form-control', required: true, placeholder: 'John Doe', minlength: 4 %>
          </div> 
          <div class="input-style input-style-2 has-icon input-required mt-4">
              <i class="input-icon fa fa-phone"></i>
              <span class="color-highlight input-style-1-active">Phone Number</span>
              <%= f.text_field :phone, placeholder: "2628547895", class: "form-control", value: current_user&.phone ? current_user&.phone : @phone, required: true %>
          </div> 
          <div class="input-style input-style-2 has-icon input-required">
              <i class="input-icon fa fa-university"></i>
              <span class="color-highlight input-style-1-active">Account Number</span>
              <em>(required)</em>
              <%= f.number_field :account_number, class: 'form-control', required: true, placeholder: '54965485452', minlength: 4, maxlength: 16 %>
          </div> 
          <div class="input-style input-style-2 has-icon input-required mt-4">
              <i class="input-icon fa fa-lock"></i>
              <span class="color-highlight input-style-1-active">Routing Number</span>
              <em>(required)</em>
              <%= f.number_field :routing_number, class: 'form-control', required: true, placeholder: '005578166' %>
          </div> 
          
          <%= f.submit "Add Bank", class: "btn btn-full bg-green1-dark btn-m text-uppercase rounded-sm shadow-l mb-3 mt-4 font-900"%>
        <% end %>
      </div>
    </div>

